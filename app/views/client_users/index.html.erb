<h1>Listing client_users</h1>

<table>
  <tr>
    <th>Client</th>
    <th>User</th>
    <th>Is worker</th>
    <th>Can view invoices</th>
  </tr>

<% for client_user in @client_users %>
  <tr>
    <td><%=h client_user.client_id %></td>
    <td><%=h client_user.user_id %></td>
    <td><%=h client_user.is_worker %></td>
    <td><%=h client_user.can_view_invoices %></td>
    <td><%= link_to 'Show', client_user %></td>
    <td><%= link_to 'Edit', edit_client_user_path(client_user) %></td>
    <td><%= link_to 'Destroy', client_user, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New client_user', new_client_user_path %>
